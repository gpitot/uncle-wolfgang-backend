{
  "name": "north-manly-squash-backend",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "start": "npm run prod",
    "dev": "npm run watch ./src/bin/www",
    "prod": "NODE_ENV=production node ./dist/bin/www",
    "build": "npm run clean-backend && babel ./src --out-dir dist",
    "clean-backend": "rimraf dist",
    "watch": "nodemon --config ./nodemon.json --",
    "heroku-postbuild": "npm run build"
  },
  "dependencies": {
    "@babel/cli": "7.4.4",
    "@babel/core": "7.4.4",
    "@babel/node": "7.2.2",
    "@babel/plugin-proposal-class-properties": "7.5.5",
    "@babel/preset-env": "7.4.4",
    "bcrypt": "^5.0.1",
    "config": "^3.2.4",
    "cookie-parser": "1.4.4",
    "cors": "^2.8.5",
    "debug": "~2.6.9",
    "dotenv": "^8.2.0",
    "express": "4.16.4",
    "express-session": "1.17.1",
    "form-data": "^4.0.0",
    "jest": "^27.4.5",
    "jsonwebtoken": "^8.5.1",
    "node-fetch": "^2.6.1",
    "passport": "^0.4.1",
    "passport-google-oauth20": "^2.0.0",
    "pg": "^8.5.1",
    "prettier": "^2.4.1",
    "rimraf": "^2.6.3",
    "twilio": "^3.63.0",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "eslint": "^5.16.0",
    "eslint-plugin-import": "^2.17.2",
    "gulp": "^4.0.2",
    "gulp-zip": "^5.0.0",
    "husky": "^3.1.0",
    "lint-staged": "^9.5.0",
    "nodemon": "^1.19.0",
    "yargs": "^14.0.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.js": [
      "prettier --write",
      "eslint --fix",
      "git add"
    ]
  },
  "engines": {
    "node": "10.x"
  }
}
